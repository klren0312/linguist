{
	"name": "browser-addon",
	"version": "4.0.3",
	"description": "Browser addon for fast translate text and pages",
	"license": "GPL-3.0",
	"author": "Vitonsky <https://github.com/vitonsky>",
	"scripts": {
		"dev": "webpack-cli",
		"test": "jest",
		"test-fast": "TEST_FAST=true jest --maxWorkers=4",
		"clean": "rm -rf ./build",
		"packAll": "cd build && ../scripts/zipAll.sh && cd ..",
		"test:buildArchives": "cd build && ../scripts/testBuildArchives.sh && cd ..",
		"build:all": "npm run clean && npm run build:firefox && npm run build:chromium && npm run build:chrome && npm run packAll && npm run test:buildArchives",
		"build:dev": "export EXT_TARGET=firefox && webpack-cli -w",
		"build:tokens": "themekit build -c themekit.config.json",
		"build:firefox": "export NODE_ENV=production && export EXT_TARGET=firefox && webpack-cli",
		"build:chromium": "export NODE_ENV=production && export EXT_TARGET=chromium && webpack-cli",
		"build:chrome": "export NODE_ENV=production && export EXT_TARGET=chrome && webpack-cli",
		"run:mobile-firefox": "ADB_DEVICE_TO_DEBUG=$(cat .ADB_DEVICE) && cd build/dev/firefox && web-ext run -t firefox-android --adb-device \"$ADB_DEVICE_TO_DEBUG\" --firefox-apk org.mozilla.fenix",
		"release:patch": "npm version patch",
		"prettify": "prettier --write \"{*,**/*}.{ts,tsx,js,jsx}\" && eslint --fix \"{*,**/*}.{ts,tsx,js,jsx}\" && stylelint --fix \"{*,**/*}.css\"",
		"prepare": "husky install"
	},
	"dependencies": {
		"@bem-react/classname": "^1.5.10",
		"@bem-react/core": "^3.0.1",
		"@popperjs/core": "^2.11.5",
		"@react-aria/interactions": "^3.9.1",
		"@react-aria/utils": "^3.13.1",
		"@translate-tools/core": "^0.2.10",
		"colord": "^2.9.1",
		"fp-ts": "^2.12.1",
		"google-tts-api": "^2.0.2",
		"idb": "^7.1.1",
		"io-ts": "^2.2.16",
		"lodash": "^4.17.21",
		"papaparse": "^5.3.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.2",
		"react-elegant-ui": "^0.3.3",
		"react-infinite-scroller": "^1.2.6",
		"react-shadow": "^18.6.2",
		"use-debounce": "^8.0.4",
		"webextension-polyfill": "^0.10.0"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.18.6",
		"@babel/preset-react": "^7.18.6",
		"@commitlint/cli": "^12.1.4",
		"@commitlint/config-conventional": "^12.1.4",
		"@svgr/webpack": "^5.5.0",
		"@types/jest": "^29.2.2",
		"@types/lodash": "^4.14.182",
		"@types/node": "^15.12.1",
		"@types/papaparse": "^5.3.2",
		"@types/react": "^17.0.47",
		"@types/react-dom": "^17.0.17",
		"@types/react-infinite-scroller": "^1.2.3",
		"@types/resize-observer-browser": "^0.1.7",
		"@types/webextension-polyfill": "^0.9.1",
		"@typescript-eslint/eslint-plugin": "^4.8.1",
		"@typescript-eslint/parser": "^4.8.1",
		"@yandex/themekit": "^1.6.7",
		"babel-loader": "^8.2.5",
		"convert-svg-to-png": "^0.5.0",
		"copy-webpack-plugin": "^6.4.1",
		"crx": "^5.0.1",
		"css-loader": "^5.2.6",
		"eslint": "^7.27.0",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"fake-indexeddb": "^4.0.0",
		"husky": "^7.0.1",
		"jest": "^29.3.1",
		"jest-localstorage-mock": "^2.4.22",
		"jest-webextension-mock": "^3.7.22",
		"lint-staged": "^11.0.0",
		"mini-css-extract-plugin": "^1.6.0",
		"postcss-loader": "^4.3.0",
		"postcss-rem-to-pixel": "^4.1.2",
		"prettier": "^2.7.1",
		"stylelint": "^13.13.1",
		"stylelint-config-standard": "^22.0.0",
		"stylelint-no-unsupported-browser-features": "^5.0.3",
		"ts-jest": "^29.0.3",
		"ts-loader": "^8.4.0",
		"typescript": "^4.7.4",
		"web-ext": "^6.8.0",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0"
	}
}
